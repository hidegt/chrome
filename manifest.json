{
  "manifest_version" : 2,
  "name" : "Hide Alert",
  "version" : "1.0.0",
  "description": "Practice Chrome Extension",
  "browser_action" : {
    "default_title" : "Hide Alert",
    "default_popup" : "popup.html"
  },
  "content_scripts" : [
    {
      "matches" : ["https://www.youtube.com/*"],
      "js" : ["script.js"]
    }
  ]
}